{
    "name": "bell-app",
    "version": "0.0.1",
    "description": "bell for 2nd round of RTO",
    "scripts": {
        "dev": "NODE_ENV=development nodemon -w src --excec \"babel-node src --presets es2015,stage-0\"",
        "build": "babel src -s -D -d dist --presets es2015,stage-0",
        "start": "NODE_ENV=production pm2 start dist",
        "prestart": "npm run -s build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "eslintConfig": {
        "parserOPtions": {
            "ecmaVersion": 7,
            "sourceType": "module"
        },
        "env": {
            "node": true
        },
        "rules": {
            "no-console": 0,
            "no-unused-vars": 1
        }
    }
    "dependencies": {
        "body-parser": "^1.18.3",
        "express": "^4.16.4",
        "express-jwt": "^5.3.1",
        "jsonwebtoken": "^8.4.0",
        "passport": "^0.4.0",
        "passport-local-mongoose": "^5.0.1",
        "socket.io": "^2.2.0"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-eslint": "^10.0.1",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "eslint": "^5.14.0",
        "nodemon": "^1.18.10"
    }
}
